<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:orientation="vertical"
    android:columnCount="2"
    tools:context=".Hello">

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:editable="false"
        android:text="GPS" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:layout_column="1"
        android:layout_row="0"
        android:text="gps status"
        android:id="@id/gps" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Lat/Lng"
        android:layout_column="0"
        android:layout_row="1" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="latlng"
        android:id="@id/latlng"
        android:layout_column="1"
        android:layout_row="1" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Elevation"
        android:layout_column="0"
        android:layout_row="2" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/elevation"
        android:text="elevation "
        android:layout_column="1"
        android:layout_row="2" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Mag. Field"
        android:layout_column="0"
        android:layout_row="3" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/magfield"
        android:text="magfield"
        android:layout_column="1"
        android:layout_row="3" />


    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Pressure"
        android:layout_column="0"
        android:layout_row="4" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/pressure"
        android:text="pressure"
        android:layout_column="1"
        android:layout_row="4" />


    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Rel. Humidity"
        android:layout_column="0"
        android:layout_row="5" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/relhum"
        android:text="relhum"
        android:layout_column="1"
        android:layout_row="5" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Amb. Temp"
        android:layout_column="0"
        android:layout_row="6" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/temp"
        android:text="temp"
        android:layout_column="1"
        android:layout_row="6" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Proximity"
        android:layout_column="0"
        android:layout_row="7" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/proximity"
        android:text="proximity"
        android:layout_column="1"
        android:layout_row="7" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Light"
        android:layout_column="0"
        android:layout_row="8" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/light"
        android:text="light"
        android:layout_column="1"
        android:layout_row="8" />


    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Linear Accel."
        android:layout_column="0"
        android:layout_row="9" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/linaccel"
        android:text="linaccel"
        android:layout_column="1"
        android:layout_row="9" />


    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Acceleration"
        android:layout_column="0"
        android:layout_row="10" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/acceleration"
        android:text="accel "
        android:layout_column="1"
        android:layout_row="10" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:text="Gravity"
        android:layout_column="0"
        android:layout_row="11" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:editable="false"
        android:id="@id/gravity"
        android:text="gravity"
        android:layout_column="1"
        android:layout_row="11" />


</GridLayout>
